# General Utility
# ---------------

OR <- _ [Oo][Rr] ( ':' / &ws )

LBRACKET <- _ '['
RBRACKET <- _ ']'
LPAREN <- _ '('
RPAREN <- _ ')'
BAR <- _ '|'
COMMA <- _ ','

line <- word+ newline / word+ eof
word <- _ wx+
wx <- [^ \t\r\n]

blankline <- &newline newline
newline <- _ eol

_ <- ws*
ws <- [ \t]
eol <- [\r]? [\n] / [\r]
eof <- [ \t\r\n]* !.

